{
  "name": "airbag-api-node",
  "version": "1.0.0",
  "target": "esnext",
  "module": "commonjs",
  "main": "./src/index.ts",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register ./src/index.ts",
    "build": "tsc && tsc-alias",
    "clean": "rimraf dist",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d src/database/sql/connection.ts",
    "migration:generate": "npm run typeorm migration:generate -p src/database/sql/migrations/base_data",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "test": "jest"
  },
  "author": "jesuscalderon2708@gmail.com",
  "license": "MIT",
  "description": "Airbag API Node.js",
  "engines": {
    "node": ">=18.18.0"
  },
  "_moduleAliases": {
    "@routes/*": [
      "src/routes/*"
    ],
    "@controllers/*": [
      "src/controllers/*"
    ],
    "@utils/*": [
      "src/utils/*"
    ],
    "@schemas/*": [
      "src/schemas/*"
    ],
    "@middleware/*": [
      "src/middleware/*"
    ],
    "@database/*": [
      "src/database/*"
    ],
    "@utils": [
      "src/utils"
    ]
  },
  "dependencies": {
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.15",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.0.3",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongodb": "^6.9.0",
    "mongoose": "^8.6.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.15",
    "pg": "^8.12.0",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@shopify/eslint-plugin": "^46.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.6",
    "@types/node": "^18.14.2",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  }
}
